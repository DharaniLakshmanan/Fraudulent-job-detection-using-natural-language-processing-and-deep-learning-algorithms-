      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "DmWk_V26CQeP"
      },
      "outputs": [],
      "source": [
        "def extractComponent(index, value):\n",
        "    if isinstance(value, str):\n",
        "        items = [item.strip() for item in value.split(',')]\n",
        "        return np.nan if len(items) <= index or len(items[index]) == 0 else items[index]\n",
        "    else:\n",
        "        return np.nan"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "YHHpuy5BCQho"
      },
      "outputs": [],
      "source": [
        "# break location into country state and city\n",
        "df['country'] = df['location'].apply(lambda x: extractComponent(0, x))\n",
        "df['state'] = df['location'].apply(lambda x: extractComponent(1, x))\n",
        "df['city'] = df['location'].apply(lambda x: extractComponent(2, x))\n",
        "df = df.drop(['location'], axis=1)\n"
      ]
