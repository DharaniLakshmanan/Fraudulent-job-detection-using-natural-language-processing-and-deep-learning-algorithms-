      "source": [
        "from tensorflow.keras.preprocessing.text import Tokenizer\n",
        "\n",
        "vocab_size = 1000\n",
        "\n",
        "\n",
        "def createTokenizedSequences(se):\n",
        "    tokenizer = Tokenizer(\n",
        "        num_words=vocab_size,\n",
        "        lower=True,\n",
        "        oov_token=\"<OOV>\",\n",
        "        filters='!\"#$%&()*+,-./:;<=>?@[\\\\]^_`{|}~\\t\\n\\r')\n",
        "    tokenizer.fit_on_texts(se.dropna().values)\n",
        "    return tokenizer.texts_to_sequences(se.apply(lambda x: x if isinstance(x, str) else ''))"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "R-iKjjV9Elps"
      },
      "outputs": [],
      "source": [
        "title_seq = createTokenizedSequences(df['title'])\n",
        "description_seq = createTokenizedSequences(df['description'])\n",
        "requirements_seq = createTokenizedSequences(df['requirements'])\n",
        "benefits_seq = createTokenizedSequences(df['benefits'])"
      ]
    },
