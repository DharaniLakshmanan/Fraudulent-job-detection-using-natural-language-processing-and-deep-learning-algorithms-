            "needs_background": "light"
          }
        }
      ],
      "source": [
        "df['title_parentheses'] = df['title'].apply(lambda x: has_parentheses(x))\n",
        "df['title_parentheses'].apply(lambda x: str(x)).value_counts().plot(kind='bar')\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "ce2PsMCHDQT3"
      },
      "outputs": [],
      "source": [
        "def get_range(upper, value):\n",
        "    if isinstance(value, str):\n",
        "        items = [item.strip() for item in value.split('-')]\n",
        "        if len(items) == 1:\n",
        "            return float(items[0])\n",
        "        return float(items[1 if upper else 0])\n",
        "    else:\n",
        "        return np.nan"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 295
        },
        "id": "PmTr0FQzDQWC",
