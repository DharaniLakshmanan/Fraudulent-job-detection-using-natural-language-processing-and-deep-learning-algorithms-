        "text_input=Input(shape=(X_train.shape[1],1,))\n",
        "text_lstm_1=GRU(100,activation='relu',kernel_initializer='he_normal', kernel_regularizer=regularizers.L2(1e-4))(text_input)\n",
        "dense_layer=Dense(10,activation='relu')(text_lstm_1)\n",
        "output=Dense(1,activation='sigmoid')(dense_layer)\n",
        "\n",
        "model=Model(inputs=[text_input],outputs=[output])"
