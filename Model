    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "BTETkanZDQgT"
      },
      "outputs": [],
      "source": [
        "df['salary_upper_log_norm'] = df['salary_range']\\\n",
        "    .apply(lambda x: get_range(False, x))\\\n",
        "    .apply(lambda x: np.log10(x + 1))\\\n",
        "    .apply(lambda x: min(x / 10.0, 1.0)) # data is in the range 0 - 10\n",
        "    \n",
        "df['salary_upper_log_norm'] = df['salary_range']\\\n",
        "    .apply(lambda x: get_range(False, x))\\\n",
        "    .apply(lambda x: np.log10(x + 1))\\\n",
        "    .apply(lambda x: min(x / 10.0, 1.0)) # data is in the range 0 - 10\n",
        "    \n",
        "df = df.drop(['salary_range'], axis=1)"
      ]
