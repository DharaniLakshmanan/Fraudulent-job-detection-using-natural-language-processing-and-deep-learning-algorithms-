      "outputs": [],
      "source": [
        "def percentage_of_caps(value):\n",
        "    if isinstance(value, str):\n",
        "        caps_count = len(re.findall(r'[A-Z]',value))\n",
        "        lower_case_count = len(re.findall(r'[a-z]',value))\n",
        "        return 0 if caps_count == 0 else (caps_count / float(caps_count + lower_case_count))\n",
        "    else:\n",
        "        0\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
