      "source": [
        "def has_parentheses(value):\n",
        "    if isinstance(value, str):\n",
        "        if len(re.findall(r'[\\(\\)\\[\\]]',value)) == 0:\n",
        "            return False\n",
        "        else:\n",
        "            return True\n",
        "    else:\n",
        "        return False"
      ]
    },
